  "homepage": "https://github.com/Novo-Ideation-Inc/ng2-dynamic-forms#readme",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0",
    "codelyzer": "0.0.28",
    "rxjs": "^5.0.0-beta.12",
    "tslint": "^3.15.1",
